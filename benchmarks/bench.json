{
  "repeat": 5,
  "reference": "dask-scheduler",
  "configurations": {
    "$product": {
      "cluster": {"$ref": "clusters"},
      "usecase": {"$ref": "usecases"}
    }
  },
  "clusters": {
    "$product": {
      "scheduler": ["$BUILD/target/release/rsds", "dask-scheduler"],
      "workers": {
        "$product": {
          "count": [1, 7],
          "cores": [24]
        }
      }
    }
  },
  "usecases": {"$+": [
    {"$ref": "xarray"},
    {"$ref": "tree"},
    {"$ref": "numpy"},
    {"$ref": "merge"},
    {"$ref": "dataframe"}
  ]},
  "xarray": {
    "$product": {
      "function": ["xarray"],
      "args": [5, 25]
    }
  },
  "tree": {
    "$product": {
      "function": ["tree"],
      "args": [10, 15]
    }
  },
  "numpy": {
    "$product": {
      "function": ["numpy"],
      "args": [5000, 10000, 20000]
    }
  },
  "merge": {
    "$product": {
      "function": ["merge"],
      "args": [5000, 10000, 15000, 25000]
    }
  },
  "dataframe": {
    "$product": {
      "function": ["dataframe"],
      "args": [1, 5, 10, 30]
    }
  }
}
