{
  "repeat": 3,
  "reference": "dask",
  "configurations": {
    "$product": {
      "cluster": {"$ref": "clusters"},
      "usecase": {"$ref": "usecases"}
    }
  },
  "clusters": {
    "$product": {
      "scheduler": [
        {"$ref": "rsds-random"},
        {"$ref": "rsds-ws"},
        {"$ref": "dask"}
      ],
      "workers": {
        "$product": {
          "count": [1, 7],
          "cores": [24]
        }
      }
    }
  },
  "rsds-random": {
    "name": "rsds-random",
    "binary": "$BUILD/target/release/rsds",
    "args": ["--scheduler", "random"]
  },
  "rsds-ws": {
    "name": "rsds-ws",
    "binary": "$BUILD/target/release/rsds",
    "args": ["--scheduler", "workstealing"]
  },
  "dask": {
    "name": "dask",
    "binary": "dask-scheduler"
  },
  "usecases": {"$+": [
    {"$ref": "xarray"},
    {"$ref": "tree"},
    {"$ref": "numpy"},
    {"$ref": "merge"},
    {"$ref": "dataframe"}
  ]},
  "xarray": {
    "$product": {
      "function": ["xarray"],
      "args": [5, 25]
    }
  },
  "tree": {
    "$product": {
      "function": ["tree"],
      "args": [15]
    }
  },
  "numpy": {
    "$product": {
      "function": ["numpy"],
      "args": [20000, 50000]
    }
  },
  "merge": {
    "$product": {
      "function": ["merge"],
      "args": [10000, 15000, 25000, 50000]
    }
  },
  "dataframe": {
    "$product": {
      "function": ["dataframe"],
      "args": [30, 60]
    }
  }
}
